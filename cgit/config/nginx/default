server {
  listen 8080;
  server_name localhost;
  
  location / {
    root  /var/www/html;
    index index.html;
  }  
 
  location ~ ^/(images|javascript|js|css|media|static|statics)/ {
    root   /var/www;
    expires 30d;
  }

#  location ~ /^(.*)$/ {
#     echo "here";
#    proxy_pass http://$uri.mirror.com:8080/;
#  }

  location /github/ {
    proxy_pass http://github.mirror.com:8080/;
  }

  location /yocto/ { 
    proxy_pass http://yocto.mirror.com:8080/;
  }  
}

